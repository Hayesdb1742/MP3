*** Start Fetch Cycle  ***
 st=0	rt='[pc]-> mar'		imar rac=1 rn=3
 st=1	rt='[[mar]]-> mdr'	read
 st=2	rt='[mdr] -> ir' 	      omdr iir
 st=3 rt='[pc]+1 -> q' 	rac=1 rn=3 ib p1 oadder
 st=4 rt='[q] -> pc'	oq wac=1 wn=3
       cond='ir158' value=2 nst=100
       cond='ir158' value=3 nst=100
	cond='ir158' value=4 nst=100
	nst=10


*** If any instructions are present except except HALT or INC, a halt will occur. ***
 st=10 halt


******************************************************************************

*** Register Addressing Mode *** 
 st=20
       cond='ir118' value=2 nst=21
       cond='ir118' value=3 nst=23
       cond='ir118' value=4 nst=26

*** INC Register Addressing ***
 st=21 rt='[r] + 1 -> q'	rac=3 ib p1 oadder newv newc
 st=22 rt='[q] -> r' 		oq wac=3 newz newn
 	 nst=0
       
*** DEC Register Addressing ***
 st=23 rt='[r]->[t1]' 	rac=3 it1
 st=24 rt='[t1]-1-> q'	oa comp oadder newv newc
 st=25 rt='[q] -> r' 	oq wac=3 newz newn
       nst=0

*** NEG Register Addressing ***
 st=26 rt='0-[r]->q' 	rac=3 ib p1 oadder comp newv newc
 st=27 rt='[q]->r' 	oq wac=3 newz newn
 	 nst=0

******************************************************************************

*** Register Indirect Addressing Mode ***
 st=30 rt='[r]->mar'       	rac=3 imar
 st=31 rt='[[mar]]->mdr'	read
 st=32 rt='[mdr]->t1' 	omdr it1
 st=33
       cond='ir118' value=2 nst=34
       cond='ir118' value=3 nst=35
       cond='ir118' value=4 nst=36

*** INC Indirect Register Addressing ***
 st=34 rt='[t1]+1->[q]'		oa p1 oadder newv newc
       nst=37

*** DEC Indirect Register Addressing ***
 st=35 rt='[t1]-1->[q]'		oa comp oadder newv newc
       nst=37

*** NEG Indirect Register Addressing ***
 st=36 rt='0 - [t1]->q'		ot1 ib p1 oadder comp newv newc
       nst=37

 st=37 rt='[q]->mdr' 	oq imdr newz newn
       nst=1000

******************************************************************************

*** Auto Increment Addressing Mode ***
 st=40 rt='[r]-> mar' 	rac=3 imar
 st=41 rt='[r] + 1-> q'	rac=3 ib p1 oadder
 st=42 rt='[q]-> r' 		oq wac=3 
 st=43 rt='[[mar]]-> mdr'   read
 st=44
       cond='ir118' value=2 nst=45
       cond='ir118' value=3 nst=46
       cond='ir118' value=4 nst=48

*** INC Auto Increment Addressing ***
 st=45 rt='mdr + 1-> q'       omdr ib p1 oadder newv newc
       nst=49

*** DEC AutoIncrement Addressing ***
 st=46 rt='[mdr]->t1'         omdr it1 
 st=47 rt='[t1]-1->q'         oa comp oadder newv newc
       nst=49

*** NEG Auto Increment Addressing ***
 st=48 rt='0 - mdr-> q'		omdr ib p1 oadder comp newv newc
       nst=49

 st=49 rt='[q]-> mdr'         oq imdr newz newn
       nst=1000



******************************************************************************


*** Decrement *** **DONE***
 st=50 rt='[r]->t1' 		rac=3 it1
 st=51 rt='[t1]-1->q'		oa comp oadder 
 st=52 rt='[q]->r' 		oq wac=3 
 st=53 rt='[q]-> mar'         rac=3 imar
 st=54 rt='[[mar]]-> mdr'     read
 st=55
      cond='ir118' value=2 nst=56
      cond='ir118' value=3 nst=57
      cond='ir118' value=4 nst=59

*** INC Auto Decrement Addressing ***
 st=56 rt='mdr + 1-> q'       omdr ib p1 oadder newv newc
       nst=60

*** DEC Auto Decrement Addressing ***
 st=57 rt='[mdr]->t1'         omdr it1 
 st=58 rt='[t1]-1->q'         oa comp oadder newv newc
       nst=60

*** NEG Auto Decrement Addressing ***
 st=59 rt='0 - mdr-> q'		omdr ib p1 oadder comp newv newc
       nst=60


       

 st=60 rt='[q]-> mdr'         oq imdr newz newn
       nst=1000



******************************************************************************

*** Index addressing *** **DONE***
 st=70 rt='[mdr] -> t1'	      omdr it1
 st=71 rt='[DST] + [t1] -> q'	rac=3 ib oa oadder newv newc
 st=72 rt='[q] -> mar'		oq imar newz newn
 st=73 rt='[[mar]] -> mdr'	read
      cond='ir118' value=2 nst=74
      cond='ir118' value=3 nst=75
      cond='ir118' value=4 nst=77

*** INC for Index Addressing***
 st=74 rt='[mdr]+1->q'        omdr ib oadder p1 newv newc
      nst=78

*** DEC for Index Addressing***
 st=75 rt='[mdr]->t1'         omdr it1 
 st=76 rt='[t1]-1->q'         oa comp oadder newv newc
       nst=78

*** NEG for Index Addressing***
 st=77 rt='0 - [mdr]-> q'		omdr ib p1 oadder comp newv newc
       nst=78

 st=78 rt='[q] -> mdr'		oq imdr newz newn
       nst=1000

