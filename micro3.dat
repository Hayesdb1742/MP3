*** Absolute *** **DONE***
 st=80 rt='[mdr] -> mar'	omdr imar
 st=81 rt='[[mar]] -> mdr'	read
      cond='ir118' value=2 nst=82
      cond='ir118' value=3 nst=83
      cond='ir118' value=4 nst=85

*** INC for Index Addressing***
 st=82 rt='[mdr]+1->q'         omdr ib oadder p1 newv newc
      nst=86

*** DEC for Index Addressing***
 st=83 rt='[mdr]->t1'         omdr it1 
 st=84 rt='[t1]-1->q'         oa comp oadder newv newc
       nst=86

*** NEG for Index Addressing***
 st=85 rt='0 - [mdr]-> q'	omdr ib p1 oadder comp newv newc
       nst=86
 
 st=86 rt='[q] -> mdr'		oq imdr newz newn
       nst=1000

